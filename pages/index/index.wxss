.home {
    position: relative;
    left: 0;
    top: 0;
}

.home .ub {
    display: inline-block;
    width: auto !important;
    padding: 0;
    background: transparent;
    margin: 0;
}

.mask {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100vh;
}

.total {
    display: flex;
    align-items: center;
    justify-content: center;
    color: #fdf2a9;
    font-size: 1.1rem;
    font-weight: bold;
    font-style: italic;
    text-shadow: 0 1px #000, 1px 0 #000, -1px 0 #000, 0 -1px #000;
}

.total image {
    width: 120rpx;
    margin-left: 10rpx;
}

.item {
    display: flex;
    align-items: center;
    padding: 3% 13%;
    box-sizing: border-box;
    justify-content: space-between;
    font-size: 0.8rem;
}

.item>view:nth-child(1) {
    width: 40%;
}

.jitui {
    width: 50rpx !important;
    margin-right: 10rpx;
}

.wc {
    width: 170rpx !important;
}

.renyuan {
    display: flex;
    align-items: center;
    justify-content: space-evenly;
}

.tx {
    width: 120rpx;
    height: 120rpx;
}

.tx1 {
    width: 106rpx;
    height: 106rpx;
    border-radius: 50%;
    overflow: hidden;
    border: 3px solid #000;
}

.jiangli {
    box-sizing: border-box;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}

.num {
    position: absolute;
    bottom: 10%;
    left: 0;
    width: 100%;
    text-align: center;
    font-size: 22rpx;
}

.num span {
    color: red;
}

.jl {
    width: 180rpx !important;
    margin: 10rpx 0;
    position: relative;
}

.jlbg {
    width: 100%;
}

.luck {
    position: absolute;
    left: 25%;
    top: 15%;
    width: 90rpx;
    height: 90rpx;
}

.lname {
    position: absolute;
    left: 0;
    top: 67%;
    width: 100%;
    text-align: center;
    font-size: 24rpx;
    text-shadow: 0 1px #000, 1px 0 #000, -1px 0 #000, 0 -1px #000, 1px -1px #000, -1px 1px #000, 1px 1px #000, -1px -1px #000;
    color: #fff;
    font-style: italic;
}